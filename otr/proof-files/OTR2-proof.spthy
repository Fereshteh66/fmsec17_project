    36. dhY   = x.16^(x.18*x.21*inv((x.19*x.20)))
        x     = (x.20*inv((x.17*x.18)))
        z     = x.16^(x.21*inv((x.17*x.19)))
    
    37. dhY   = x.16^(x.18*inv((x.19*x.20)))
        x     = (x.20*x.21*inv((x.17*x.18)))
        z     = x.16^(x.21*inv((x.17*x.19)))
    
    38. dhY   = x.16^(x.19*x.21*inv(x.17))
        x     = (x.17*x.20*inv((x.18*x.19)))
        z     = x.16^(x.20*x.21*inv(x.18))
    
    39. dhY   = x.16^(x.19*x.21*inv(x.17))
        x     = (x.20*inv((x.18*x.19)))
        z     = x.16^(x.20*x.21*inv((x.17*x.18)))
    
    40. dhY   = x.16^(x.21*inv((x.18*x.19)))
        x     = (x.19*x.20*inv(x.17))
        z     = x.16^(x.20*x.21*inv((x.17*x.18)))
    
    41. dhY   = x.17^(x.19*x.23*inv((x.20*x.21)))
        x     = (x.21*x.22*inv((x.18*x.19)))
        z     = x.17^(x.22*x.23*inv((x.18*x.20)))
    
    42. dhY   = z.18^x.26
        x     = inv(x.26)
        z     = z.18
    
    43. dhY   = z.19^(x.27*inv(x.28))
        x     = (x.28*inv(x.27))
        z     = z.19
    
    44. dhY   = x.35^x.36
        x     = x.22
        z     = x.35^(x.22*x.36)
    
    45. dhY   = x.36^inv((x.23*x.38))
        x     = x.23
        z     = x.36^inv(x.38)
    
    46. dhY   = x.36^(x.38*inv(x.23))
        x     = x.23
        z     = x.36^x.38
    
    47. dhY   = x.37^(x.40*inv((x.24*x.39)))
        x     = x.24
        z     = x.37^(x.40*inv(x.39))
  */

restriction unique:
  "∀ x #i #j.
    ((Restriction_Unique( x ) @ #i) ∧ (Restriction_Unique( x ) @ #j)) ⇒
    (#i = #j)"
  // safety formula

lemma executable:
  exists-trace
  "((∃ A B K #i #j.
      (Running( A, B, <'I', 'R', K> ) @ #i) ∧
      (Commit( B, A, <'I', 'R', K> ) @ #j)) ∧
    (¬(∃ A #i. Reveal( A ) @ #i))) ∧
   (¬(∃ A #i #j.
       ((Executable_Unique( A ) @ #i) ∧ (Executable_Unique( A ) @ #j)) ∧
       (¬(#i = #j))))"
/*
guarded formula characterizing all satisfying traces:
"(∃ A B K #i #j.
   (Running( A, B, <'I', 'R', K> ) @ #i) ∧
   (Commit( B, A, <'I', 'R', K> ) @ #j)) ∧
 (∀ A #i. (Reveal( A ) @ #i) ⇒ ⊥) ∧
 (∀ A #i #j.
   (Executable_Unique( A ) @ #i) ∧ (Executable_Unique( A ) @ #j)
  ⇒
   #i = #j)"
*/
simplify
solve( Running( A, B, <'I', 'R', K> ) @ #i )
  case R_2_sendDhY
  solve( St_R_1( A, id, skB, B, pkA, dhX ) ▶₀ #i )
    case R_1_recvDhX
    solve( Commit( $A.1, $A, <'I', 'R', K> ) @ #j )
      case I_2_recvDhY
      solve( St_I_1( $A.1, id.1, skA, $A, pk(skB), x ) ▶₀ #j )
        case I_1_sendDhX
        solve( !KU( aenc(dhX, ~newKey.1) ) @ #vk.3 )
          case I_1_sendDhX
          solve( !KU( pk(~newKey.1) ) @ #vk.4 )
            case I_1_sendDhX
            solve( !KU( aenc('g'^~y, ~newKey) ) @ #vk.8 )
              case R_2_sendDhY
              solve( !KU( pk(~newKey) ) @ #vk.9 )
                case Init_PKI
                SOLVED // trace found
              qed
            qed
          qed
        qed
      qed
    qed
  qed
qed

lemma secrecy:
  all-traces
  "∀ A x #i.
    (Secret( A, x ) @ #i) ⇒
    ((¬(∃ #j. K( x ) @ #j)) ∨
     (∃ X #r. (Reveal( X ) @ #r) ∧ (Honest( X ) @ #i)))"
/*
guarded formula characterizing all counter-examples:
"∃ A x #i.
  (Secret( A, x ) @ #i)
 ∧
  (∃ #j. (K( x ) @ #j)) ∧
  (∀ X #r. (Reveal( X ) @ #r) ∧ (Honest( X ) @ #i) ⇒ ⊥)"
*/
simplify
solve( Secret( A, x ) @ #i )
  case I_2_recvDhY
  solve( St_I_1( A, id, skA, B, pk(skB), x ) ▶₀ #i )
    case I_1_sendDhX
    solve( !KU( aenc(dhY, ~newKey.1) ) @ #vk.4 )
      case I_1_sendDhX
      solve( !KU( 'g'^(~x*~x.1) ) @ #vk.1 )
        case cexp
        by solve( !KU( ~x ) @ #vk.8 )
      qed
    next
      case R_2_sendDhY
      solve( !KU( 'g'^(~x*~y) ) @ #vk.1 )
        case cexp
        by solve( !KU( ~x ) @ #vk.13 )
      qed
    next
      case c_aenc
      solve( !KU( ~newKey.1 ) @ #vk.7 )
        case Reveal_SK
        by contradiction /* from formulas */
      qed
    qed
  qed
next
  case R_2_sendDhY
  solve( St_R_1( A, id, skB, A.1, pkA, dhX ) ▶₀ #i )
    case R_1_recvDhX
    solve( !KU( aenc(dhX, ~newKey.1) ) @ #vk.4 )
      case I_1_sendDhX
      solve( !KU( 'g'^(~x*~y) ) @ #vk.1 )
        case cexp
        by solve( !KU( ~x ) @ #vk.8 )
      qed
    next
      case R_2_sendDhY
      solve( !KU( 'g'^(~y*~y.1) ) @ #vk.1 )
        case cexp
        by solve( !KU( ~y ) @ #vk.12 )
      qed
    next
      case c_aenc
      solve( !KU( ~newKey.1 ) @ #vk.7 )
        case Reveal_SK
        by contradiction /* from formulas */
      qed
    qed
  qed
qed

lemma secrecy_PFS:
  all-traces
  "∀ A x #i.
    (Secret( A, x ) @ #i) ⇒
    ((¬(∃ #j. K( x ) @ #j)) ∨
     (∃ X #r. ((Reveal( X ) @ #r) ∧ (Honest( X ) @ #i)) ∧ (#r < #i)))"
/*
guarded formula characterizing all counter-examples:
"∃ A x #i.
  (Secret( A, x ) @ #i)
 ∧
  (∃ #j. (K( x ) @ #j)) ∧
  (∀ X #r. (Reveal( X ) @ #r) ∧ (Honest( X ) @ #i) ⇒ ¬(#r < #i))"
*/
simplify
solve( Secret( A, x ) @ #i )
  case I_2_recvDhY
  solve( St_I_1( A, id, skA, B, pk(skB), x ) ▶₀ #i )
    case I_1_sendDhX
    solve( !KU( aenc(dhY, ~newKey.1) ) @ #vk.4 )
      case I_1_sendDhX
      solve( !KU( 'g'^(~x*~x.1) ) @ #vk.1 )
        case cexp
        by solve( !KU( ~x ) @ #vk.8 )
      qed
    next
      case R_2_sendDhY
      solve( !KU( 'g'^(~x*~y) ) @ #vk.1 )
        case cexp
        by solve( !KU( ~x ) @ #vk.13 )
      qed
    next
      case c_aenc
      solve( !KU( ~newKey.1 ) @ #vk.7 )
        case Reveal_SK
        by contradiction /* from formulas */
      qed
    qed
  qed
next
  case R_2_sendDhY
  solve( St_R_1( A, id, skB, A.1, pkA, dhX ) ▶₀ #i )
    case R_1_recvDhX
    solve( !KU( aenc(dhX, ~newKey.1) ) @ #vk.4 )
      case I_1_sendDhX
      solve( !KU( 'g'^(~x*~y) ) @ #vk.1 )
        case cexp
        by solve( !KU( ~x ) @ #vk.8 )
      qed
    next
      case R_2_sendDhY
      solve( !KU( 'g'^(~y*~y.1) ) @ #vk.1 )
        case cexp
        by solve( !KU( ~y ) @ #vk.12 )
      qed
    next
      case c_aenc
      solve( !KU( ~newKey.1 ) @ #vk.7 )
        case Reveal_SK
        by contradiction /* from formulas */
      qed
    qed
  qed
qed

lemma noninjectiveagreementINITIATOR:
  all-traces
  "∀ a b t #i.
    (Commit( a, b, <'I', 'R', t> ) @ #i) ⇒
    ((∃ #j. Running( b, a, <'I', 'R', t> ) @ #j) ∨
     (∃ X #r. (Reveal( X ) @ #r) ∧ (Honest( X ) @ #i)))"
/*
guarded formula characterizing all counter-examples:
"∃ a b t #i.
  (Commit( a, b, <'I', 'R', t> ) @ #i)
 ∧
  (∀ #j. (Running( b, a, <'I', 'R', t> ) @ #j) ⇒ ⊥) ∧
  (∀ X #r. (Reveal( X ) @ #r) ∧ (Honest( X ) @ #i) ⇒ ⊥)"
*/
simplify
solve( Commit( a, b, <'I', 'R', t> ) @ #i )
  case I_2_recvDhY
  solve( St_I_1( a, id, skA, b, pk(skB), x ) ▶₀ #i )
    case I_1_sendDhX
    solve( !KU( pk(~newKey.1) ) @ #vk.4 )
      case I_1_sendDhX
      by sorry
    next
      case Init_PKI
      solve( !KU( aenc(dhY, ~newKey.1) ) @ #vk.4 )
        case I_1_sendDhX
        SOLVED // trace found
      next
        case R_2_sendDhY
        solve( !KU( pk(~newKey.2) ) @ #vk.9 )
          case I_1_sendDhX
          by sorry
        next
          case Init_PKI
          solve( !KU( aenc(dhX, ~newKey.2) ) @ #vk.9 )
            case I_1_sendDhX
            SOLVED // trace found
          next
            case R_2_sendDhY
            by sorry
          next
            case c_aenc
            by sorry
          qed
        next
          case R_2_sendDhY
          by sorry
        next
          case c_pk
          by sorry
        qed
      next
        case c_aenc
        by sorry
      qed
    next
      case R_2_sendDhY
      by sorry
    next
      case c_pk
      by sorry
    qed
  qed
qed

lemma injectiveagreementINITIATOR:
  all-traces
  "∀ a b t #i.
    (Commit( a, b, <'I', 'R', t> ) @ #i) ⇒
    ((∃ #j.
       ((Running( b, a, <'I', 'R', t> ) @ #j) ∧ (#j < #i)) ∧
       (¬(∃ a2 b2 #i2.
           (Commit( a2, b2, <'I', 'R', t> ) @ #i2) ∧ (¬(#i2 = #i))))) ∨
     (∃ X #r. (Reveal( X ) @ #r) ∧ (Honest( X ) @ #i)))"
/*
