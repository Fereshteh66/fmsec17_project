    30. dhY   = x.15^(x.18*inv(x.16))
        x     = (x.19*inv((x.17*x.18)))
        z     = x.15^(x.19*inv((x.16*x.17)))
    
    31. dhY   = x.15^(x.19*inv(x.17))
        x     = (x.18*inv(x.16))
