        x     = inv(x.14)
        z     = x.13^inv((x.14*x.15))
    
     7. dhY   = x.13^(x.14*x.15)
        x     = inv(x.14)
        z     = x.13^x.15
    
     8. dhY   = x.14^x.15
        x     = (x.17*inv((x.15*x.16)))
        z     = x.14^(x.17*inv(x.16))
    
     9. dhY   = x.14^inv(x.15)
        x     = (x.17*inv(x.16))
        z     = x.14^(x.17*inv((x.15*x.16)))
    
    10. dhY   = x.14^inv((x.15*x.16))
        x     = (x.16*x.17)
        z     = x.14^(x.17*inv(x.15))
    
    11. dhY   = x.14^inv((x.16*x.17))
        x     = (x.17*inv(x.15))
        z     = x.14^inv((x.15*x.16))
    
