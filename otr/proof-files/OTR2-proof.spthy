    32. dhY   = x.15^(x.19*inv((x.16*x.17)))
        x     = (x.17*x.18)
        z     = x.15^(x.18*x.19*inv(x.16))
    
    33. dhY   = x.15^(x.19*inv((x.17*x.18)))
        x     = (x.18*inv(x.16))
        z     = x.15^(x.19*inv((x.16*x.17)))
    
    34. dhY   = z.15^inv(x.13)
        x     = x.13
        z     = z.15
    
    35. dhY   = x.16^(x.17*x.21*inv((x.18*x.19)))
