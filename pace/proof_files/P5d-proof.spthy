lemma executable:
  exists-trace
  "∃ #i. (Finish( ) @ #i) ∧ (¬(∃ A #j. Reveal( A ) @ #j))"
/*
guarded formula characterizing all satisfying traces:
"∃ #i. (Finish( ) @ #i) ∧ ∀ A #j. (Reveal( A ) @ #j) ⇒ ⊥"
*/
simplify
solve( St_I_4( A, ~id, kAB, s, x, Y, 'p', B ) ▶₀ #i )
  case I_3_send
  solve( !KU( mac(map(~s, 'p')^~x, h(z)) ) @ #vk )
    case R_4_send
    solve( !KU( mac(map(s.1, 'p')^~y, h(map(~s, 'p')^(~x*~y)))
           ) @ #vk.2 )
      case I_3_send
      solve( !KU( senc(~s, h(~shk.1)) ) @ #vk.7 )
        case I_1_send
